<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Participant.
    -->
    <changeSet id="20190321202836-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="free_id"
                                 baseTableName="participant"
                                 constraintName="fk_participant_free_id"
                                 referencedColumnNames="id"
                                 referencedTableName="free"/>

        <addForeignKeyConstraint baseColumnNames="premium_id"
                                 baseTableName="participant"
                                 constraintName="fk_participant_premium_id"
                                 referencedColumnNames="id"
                                 referencedTableName="premium"/>

        <addForeignKeyConstraint baseColumnNames="sponsor_id"
                                 baseTableName="participant"
                                 constraintName="fk_participant_sponsor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sponsor"/>

    </changeSet>
</databaseChangeLog>
