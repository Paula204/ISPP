<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Free.
    -->
    <changeSet id="20190321202831-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="free"
                                 constraintName="fk_free_actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"/>

        <addForeignKeyConstraint baseColumnNames="free_id"
                                 baseTableName="free_tournament"
                                 constraintName="fk_free_tournament_free_id"
                                 referencedColumnNames="id"
                                 referencedTableName="free"/>
        <addForeignKeyConstraint baseColumnNames="tournament_id"
                                 baseTableName="free_tournament"
                                 constraintName="fk_free_tournament_tournament_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tournament"/>
        
    </changeSet>
</databaseChangeLog>
