<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Premium.
    -->
    <changeSet id="20190321202830-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="premium"
                                 constraintName="fk_premium_actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"/>

        <addForeignKeyConstraint baseColumnNames="premium_id"
                                 baseTableName="premium_tournament"
                                 constraintName="fk_premium_tournament_premium_id"
                                 referencedColumnNames="id"
                                 referencedTableName="premium"/>
        <addForeignKeyConstraint baseColumnNames="tournament_id"
                                 baseTableName="premium_tournament"
                                 constraintName="fk_premium_tournament_tournament_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tournament"/>
        
    </changeSet>
</databaseChangeLog>
